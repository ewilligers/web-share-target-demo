{
  "name": "WebShareTargetDemo",
  "share_target": {
    "url_template": "share-target-destination.html?received_title={title}&received_text={text}&received_url={url}",

    "action": "share-target-destination.html",
    "method": "POST",
    "params": {
      "title": "received_title",
      "text": "received_text",
      "url": "received_url",
      "file": [
        {
          "name": "received_file",
          "accept": "*"
        }
      ]
    }
  }
}
